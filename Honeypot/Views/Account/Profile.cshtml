@using Honeypot.ViewModels.Account
@model Honeypot.ViewModels.Account.ProfileViewModel

<table class="table table-sm table-dark table-bordered table-hover" style="border:2px solid black">
    <tbody>
        <tr>
            <th scope="row">Username</th>
            <td>@Model.Username</td>
            <td>Edit</td>
        </tr>
        <tr>
            <th scope="row">First Name</th>
            <td>@Model.FirstName</td>
            <td>Edit</td>
        </tr>
        <tr>
            <th scope="row">Last Name</th>
            <td>@Model.LastName</td>
            <td>Edit</td>
        </tr>
        <tr>
            <th scope="row">Email</th>
            <td>@Model.Email</td>
            <td>Edit</td>
        </tr>
    </tbody>
</table>